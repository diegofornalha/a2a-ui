# Copie este arquivo para .env e configure suas chaves de API

# API Keys (opcional - os agentes funcionam com dados simulados)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_API_KEY=your-google-api-key

# Configurações do Sistema
LOG_LEVEL=INFO
MAX_RETRIES=3
TIMEOUT_SECONDS=30

# URLs dos Agentes (use os valores padrão ou customize)
ORCHESTRATOR_URL=http://localhost:8001
ESTRATEGISTA_URL=http://localhost:8002
CRIATIVO_URL=http://localhost:8003
COPYWRITER_URL=http://localhost:8004
OTIMIZADOR_URL=http://localhost:8005

# Configurações de Performance
MAX_CONCURRENT_TASKS=5
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600

# Configurações de Debug
DEBUG_MODE=false
SAVE_AGENT_RESPONSES=true
RESPONSE_DIR=./responses